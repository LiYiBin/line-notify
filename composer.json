{
    "name": "liyibin/line-notify",
    "description": "Line notify for php or Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LYB",
            "email": "liyibin.tw@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LiYiBin\\LineNotify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LiYiBin\\LineNotify\\Tests\\": "tests"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiYiBin\\LineNotify\\LineNotifyServiceProvider"
            ],
            "aliases": {
                "LineNotify": "LiYiBin\\LineNotify\\Facades\\LineNotify"
            }
        }
    }
}
